	<!-- Replicate Glitter Medicine -->
	
	<RecipeDef ParentName="MakeFromOmniGelMK3Recipe">
		<defName>Replicate_MK3_GlitterMedicineFromOmniGel</defName>
		<label>Upgrade to Glitter Medicine with Omni-Gel</label>
		<description>Upgrades Medicine to Glitterworld Medicine with Omni-Gel.</description>
		<jobString>Creating Glitterworld Medicine from Omni-Gel and Medicine.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>OmniGel</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>MedicineIndustrial</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<products>
			<MedicineUltratech>1</MedicineUltratech>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MedicineIndustrial</li>
				<li>OmniGel</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

