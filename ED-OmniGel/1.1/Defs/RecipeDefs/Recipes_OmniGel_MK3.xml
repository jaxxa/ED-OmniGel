<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef Name="MakeFromOmniGelMK3Recipe" ParentName="MakeFromOmniGelBase" Abstract="True" >
	  <recipeUsers>
        <li>Table_MK3_Replicator</li>
      </recipeUsers>
		<workAmount>500</workAmount>
	</RecipeDef>


	<!-- Replicate Plasteel -->
	<RecipeDef ParentName="MakeFromOmniGelMK3Recipe">
		<defName>Replicate_MK3_PlasteelFromOmniGel</defName>
		<label>Replicate Plasteel from Omni-Gel</label>
		<description>Replicates Plasteel from Omni-Gel.</description>
		<jobString>Creating Plasteel from Omni-Gel.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>OmniGel</li>
					</thingDefs>
				</filter>
				<count>60</count>
			</li>
		</ingredients>
		<products>
			<Plasteel>3</Plasteel>
		</products>
	</RecipeDef>

